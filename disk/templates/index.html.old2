<html>
    <head><title>server</title></head>
    <body>
        <style>
            table{
                border="1";
                }
        </style>
        <table width="100%" border="1">
            <tr><th>servername</th><th>partitionname</th><th>bsize</th><th>fsize</th><th>sharename</th>
                <th>sharepath</th></tr>
    {% for sh in share %}
    {% for part in sh.partition_set.all %}
    {% for se in part.server_set.all %}
    <tr><td>{{se.servername}}</td><td>{{part.partitioname}}</td><td>{{part.basicsize}}</td><td>{{part.freesize}}</td>
        <td>{{sh.sharename}}</td><td>{{sh.sharepath}}</td></tr>
        {% endfor %}
        {% endfor %}
        {% endfor %}
    </table>
    </body>
</html>
